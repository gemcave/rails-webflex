<!DOCTYPE html>
<html>
  <head>
    <title>WebFlex</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
			<%= render 'layouts/header' %>
			<%= render 'layouts/sidebar' %>
			<%= yield %>
		<%= render 'layouts/footer' %>
  </body>
</html>
